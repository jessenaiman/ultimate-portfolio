{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist",
    "src/components/Solid/**/*"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "types": [
      "react",
      "react-dom"
    ],
    "paths": {
      "react": [
        "./node_modules/@types/react"
      ]
    },
    "jsxImportSource": "solid-js"
  }
}